{
  "expo": {
    "name": "pavel-id",
    "slug": "pavel-id",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pavel",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.543017744396-u6cilh79a019qbaomlokvf64032ha4rh"
        }
      }
    },
    "android": {
      "package": "com.pavel",
      "googleServicesFile": "./firebase/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Pavel-id",
    "githubUrl": "https://github.com/hikaaam/pavel-id"
  }
}
